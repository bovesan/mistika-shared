<?xml version="1.0"?>
<setup>
  <animation>
    <autoKeyState>1</autoKeyState>
    <defaultAnimationType>0</defaultAnimationType>
    <timeWarpCurves>0</timeWarpCurves>
    <trimMode>0</trimMode>
  </animation>
  <cache>
    <autoBatch>false</autoBatch>
    <device>Xfs.dev:Media_Storage:%1/%2</device>
    <stereoEye>0</stereoEye>
    <syntax>[project]/[envName][renderName][_segmentIndex].[ext]</syntax>
  </cache>
  <general>
    <import>
      <defaultMediaTC>00:00:00:00</defaultMediaTC>
    </import>
    <save>
      <autoSave>true</autoSave>
      <autoSaveNumSaves>5</autoSaveNumSaves>
      <autoSaveTime>10</autoSaveTime>
    </save>
    <userPresets/>
  </general>
  <interface>
    <checkerboard>
      <maxCheckerLuma>50</maxCheckerLuma>
      <playbackCache>true</playbackCache>
    </checkerboard>
    <general>
      <backgroundLuma>8</backgroundLuma>
      <controlCanonMonitor>false</controlCanonMonitor>
      <doubleBuffer>false</doubleBuffer>
      <hideText>false</hideText>
      <liveToVideo>true</liveToVideo>
      <magnifyFilter>1</magnifyFilter>
      <playbackResolution>RES_HIGH</playbackResolution>
      <saturation>30</saturation>
      <showLegacyFx>false</showLegacyFx>
      <stitchShowParameters>Label and Value</stitchShowParameters>
      <textOpacity>30</textOpacity>
      <textSaturation>30</textSaturation>
      <textShadows>false</textShadows>
      <trackBallSpin>false</trackBallSpin>
    </general>
    <text>
      <fontName>Open Sans [Regular]</fontName>
      <fontSize>30</fontSize>
      <uiFontName>Open Sans [Regular]</uiFontName>
      <uiFontNameList>Open Sans [Regular],Baekmuk Batang [Regular]</uiFontNameList>
      <useInputMethod>false</useInputMethod>
      <useUIFont>true</useUIFont>
      <useUnicode>false</useUnicode>
      <virtualSlateFontName>Open Sans [Regular]</virtualSlateFontName>
    </text>
    <timelinemonitor>
      <flowMonitor>true</flowMonitor>
      <raiseMonitor>true</raiseMonitor>
      <recordMonitorFullScreen>false</recordMonitorFullScreen>
      <selectMonitor>false</selectMonitor>
    </timelinemonitor>
    <timelineview>
      <audioIcons>1</audioIcons>
      <groupsTCSource>0</groupsTCSource>
      <showGraffiti>true</showGraffiti>
      <showGrid>true</showGrid>
      <showHFRHalf>false</showHFRHalf>
      <showOverlaps>true</showOverlaps>
      <showRulers>true</showRulers>
      <showTimeCode>true</showTimeCode>
      <undoViewEvents>true</undoViewEvents>
      <videoIcons>1</videoIcons>
    </timelineview>
  </interface>
  <license>
    <doNotShowExpireMessage>false</doNotShowExpireMessage>
    <expireWarningDays>10</expireWarningDays>
  </license>
  <live>
    <maxFree>00:30:00:00</maxFree>
    <nameFromCamera>false</nameFromCamera>
    <nameWithTitle>false</nameWithTitle>
  </live>
  <paths>
    <audioTools>/home/mistika/SGO Data/Media/DELIVERY/Audio_Tools</audioTools>
    <cacheBatchPath>/home/mistika/SGO Data/Projects/_BATCH_QUEUES/_CACHE</cacheBatchPath>
    <dcp>/home/mistika/SGO Data/Media/DELIVERY/DCP</dcp>
    <devCache>/home/mistika/SGO Data/Media/CACHE/PLAYBACK</devCache>
    <media>/home/mistika/SGO Data/Media</media>
    <os/>
    <paint>/home/mistika/SGO Data/Media/CACHE/PAINT</paint>
    <project>/home/mistika/SGO Data/Projects</project>
    <sequenceProxies>/home/mistika/SGO Data/Media/CACHE/PROXY</sequenceProxies>
    <shared>/home/mistika/SGO AppData/shared</shared>
  </paths>
  <private>
    <interface>
      <general>
        <checkProjectPresets>true</checkProjectPresets>
        <numLeadZeros>7</numLeadZeros>
        <textSplitter>46</textSplitter>
      </general>
    </interface>
    <paths>
      <devStorage/>
      <diskDevStorage/>
    </paths>
    <render>
      <cacheDevice>DiskCache.dev</cacheDevice>
      <renderPartial>Xfs.dev</renderPartial>
    </render>
  </private>
  <projectPreset>
    <colorSpace709/>
    <factoryPreset/>
    <pulldown/>
    <redBrightness/>
    <redColorSpace/>
    <redContrast/>
    <redDenoise/>
    <redDetail/>
    <redDrx/>
    <redExposureCompensation/>
    <redGainBlue/>
    <redGainGreen/>
    <redGainRed/>
    <redGammaCurve/>
    <redIso/>
    <redKelvin/>
    <redOlpfCompensation/>
    <redSaturation/>
    <redTint/>
  </projectPreset>
  <render>
    <options>
      <defaultRenderName>RENDER</defaultRenderName>
      <renderToVideo>false</renderToVideo>
      <saveBeforeRender>false</saveBeforeRender>
    </options>
  </render>
  <stitch>
    <externalTools>
      <hugin>
        <path/>
      </hugin>
      <ptgui>
        <path/>
      </ptgui>
      <showWindowConfigApps>false</showWindowConfigApps>
      <stitchTool>PtGui</stitchTool>
    </externalTools>
  </stitch>
  <streaming>sref="name@streaming"</streaming>
  <tangent>sref="name@tangent"</tangent>
  <timelineBehaviour>
    <dropFrame>false</dropFrame>
    <edit>
      <autoExpand>true</autoExpand>
      <autoSelection>false</autoSelection>
      <autoSnapPixels>10</autoSnapPixels>
      <autoSyncE2T>true</autoSyncE2T>
      <blipFrames>3</blipFrames>
      <constraintX>false</constraintX>
      <constraintY>false</constraintY>
      <markOffset>0</markOffset>
    </edit>
    <shuttle>
      <playSpeed>0</playSpeed>
    </shuttle>
    <transition>
      <alignment>1</alignment>
      <duration>10</duration>
    </transition>
  </timelineBehaviour>
  <timelineSettings>
    <aspectRatio>1.33333337</aspectRatio>
    <frameRate>25</frameRate>
    <imageSizeX>1920</imageSizeX>
    <imageSizeY>1080</imageSizeY>
    <ntscFields>false</ntscFields>
  </timelineSettings>
  <videoFormat>
    <aja>
      <colorSpace>YUV422</colorSpace>
    </aja>
    <dvs>
      <colorSpace>YUV422</colorSpace>
      <connector>SDI</connector>
      <editLag>5</editLag>
      <embeddedAudioIn>false</embeddedAudioIn>
      <level3g>3GB</level3g>
      <stereo3dMultichannelMode>MONO</stereo3dMultichannelMode>
    </dvs>
    <general>
      <internalVideoStorage>YUV422</internalVideoStorage>
      <stereo3d>
        <dualLink>false</dualLink>
        <mirrorLeftEye>false</mirrorLeftEye>
      </stereo3d>
      <syncSource>SYNC_INTERNAL</syncSource>
      <use3GSDI>false</use3GSDI>
      <useSDIUserData>false</useSDIUserData>
      <useVideoBoard>false</useVideoBoard>
      <videoBoardModel>DVS</videoBoardModel>
      <videoFrameRate>25</videoFrameRate>
      <videoMode>Progressive</videoMode>
      <videoResolution>1920x1080</videoResolution>
      <videoResolutionX>1920</videoResolutionX>
      <videoResolutionY>1080</videoResolutionY>
    </general>
  </videoFormat>
  <visualEditorBehaviour>
    <fadeInOutShapes>1.0</fadeInOutShapes>
    <playSync>0</playSync>
  </visualEditorBehaviour>
</setup>

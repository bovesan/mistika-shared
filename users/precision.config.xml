      <display id="2" menu="tools" option="0x8400"/>
      <display id="3" menu="tools" option="0x8500"/>
    </defaults>
    <menus>
      <group name="tools">
        <item id="0x8300" default="true"/>
        <item id="0x8400"/>
        <item id="0x8500"/>
        <item id="0x8600"/>
        <item id="0x8900"/>

      <group name="iso">
        <item id="0x8200"/>
        <item id="0x8F00"/>
        <item id="0x8100" default="true"/>
        <item id="0x8A00"/>
        <item id="0x9F00"/>
      </group>
      <group name="lock">
        <item id="0x9000" default="true"/>
        <item id="0x9400"/>
        <item id="0x9500"/>
      </group>
      <group name="mon">
        <item id="0x9A00" default="true"/>
        <item id="0x9B00"/>
        <item id="0x9C00"/>
        <item id="0x9D00"/>
      </group>
    </menus>
    <leds>
      <masterIntensity>1.0</masterIntensity>
      <default>
         <positive red="72" green="146" blue ="67" intensity="1"/>
         <default intensity="0.5"/>
         <negative red="12" green="67" blue ="146" intensity="1"/>
      </default>
      <display>
         <positive red="72" green="146" blue ="67" intensity="1"/>
         <default intensity="0.5"/>
         <negative red="12" green="67" blue ="146" intensity="1"/>
      </display>
      <overlay>

<xsd:schema xmlns:sgo="http://www.sgo.es/2017/sgo.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
 <xsd:element name="precisionDriver" sgo:visibility="0" sgo:sortOrder="0">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element name="config" sgo:visibility="0" sgo:sortOrder="0">
     <xsd:complexType>
      <xsd:sequence>
       <xsd:element name="enabled" type="xsd:boolean" sgo:visibility="0" sgo:sortOrder="0" sgo:actionType="enableSiblingWhenSelected"/>
       <xsd:element name="firstIP" type="ipAddress" sgo:visibility="0" sgo:sortOrder="1"/>
       <xsd:element name="serverIP" type="xsd:string" sgo:visibility="0" sgo:sortOrder="2" sgo:properties="readOnly"/>
      </xsd:sequence>
     </xsd:complexType>
    </xsd:element>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
</xsd:schema>
